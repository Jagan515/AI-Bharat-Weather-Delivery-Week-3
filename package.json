{
  "name": "weather-delivery-dashboard",
  "version": "1.0.0",
  "description": "Interactive dashboard analyzing weather-delivery correlations",
  "main": "index.js",
  "scripts": {
    "install-all": "npm run install-backend && npm run install-frontend",
    "install-backend": "cd backend && npm install",
    "install-frontend": "cd frontend && npm install",
    "dev": "concurrently \"npm run dev-backend\" \"npm run dev-frontend\"",
    "dev-backend": "cd backend && npm run dev",
    "dev-frontend": "cd frontend && npm start",
    "build": "cd frontend && npm run build",
    "start": "cd backend && npm start",
    "test": "./run-tests.sh",
    "test-backend": "cd backend && npm test",
    "test-frontend": "cd frontend && CI=true npm test -- --coverage --watchAll=false",
    "test-system": "./test-system.sh",
    "test-unit": "./run-tests.sh --no-system"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": [
    "weather",
    "delivery",
    "correlation",
    "dashboard",
    "analytics",
    "react",
    "nodejs",
    "statistics"
  ],
  "author": "Weather Delivery Analytics Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/weather-delivery-dashboard.git"
  }
}